    "bcryptjs": "^3.0.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "livekit-client": "^2.16.0",
    "livekit-server-sdk": "^2.14.2",
    "next": "^15.0.0",
    "next-auth": "^5.0.0-beta.25",
    "phaser": "^3.90.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
