{
  "name": "flowspace",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint .",
    "socket:dev": "cross-env NODE_ENV=development tsx server/index.ts",
    "socket:start": "node --no-warnings --import tsx server/index.ts",
    "livekit:dev": "node scripts/livekit-dev.js",
    "dev:all": "concurrently \"npm run dev\" \"npm run socket:dev\" \"npm run livekit:dev\"",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:seed": "prisma db seed",
    "db:studio": "prisma studio",
    "flow:finish": "bash scripts/flow-finish || powershell -ExecutionPolicy Bypass -File scripts/flow-finish.ps1",
    "flow:finish:docs": "bash scripts/flow-finish --docs-only || powershell -ExecutionPolicy Bypass -File scripts/flow-finish.ps1 -DocsOnly",
    "specctl": "bash scripts/specctl || powershell -ExecutionPolicy Bypass -File scripts/specctl.ps1",
    "specctl:status": "bash scripts/specctl status || powershell -ExecutionPolicy Bypass -File scripts/specctl.ps1 status",
    "specctl:verify": "bash scripts/specctl verify --level=soft || powershell -ExecutionPolicy Bypass -File scripts/specctl.ps1 verify -Level soft",
    "specctl:verify:strict": "bash scripts/specctl verify --level=strict || powershell -ExecutionPolicy Bypass -File scripts/specctl.ps1 verify -Level strict"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@auth/prisma-adapter": "^2.0.0",
    "@livekit/components-react": "^2.9.17",
    "@prisma/client": "^6.0.0",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "bcryptjs": "^3.0.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "dompurify": "3.3.1",
    "livekit-client": "^2.16.0",
    "livekit-server-sdk": "^2.14.2",
    "lucide-react": "^0.561.0",
    "next": "^15.5.9",
    "next-auth": "^5.0.0-beta.25",
    "oci-sdk": "^2.122.2",
    "phaser": "^3.90.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-resizable-panels": "^3.0.6",
    "recharts": "^3.6.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "tailwind-merge": "^2.5.0",
    "tsx": "^4.0.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.17",
    "@types/bcryptjs": "^2.4.6",
    "@types/dompurify": "3.0.5",
    "@types/node": "^20.0.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "canvas": "^3.2.0",
    "concurrently": "^9.2.1",
    "cross-env": "^10.1.0",
    "eslint": "^9.39.1",
    "eslint-config-next": "^16.0.8",
    "prisma": "^6.0.0",
    "tailwindcss": "^4.0.0",
    "typescript": "^5.0.0"
  }
}
